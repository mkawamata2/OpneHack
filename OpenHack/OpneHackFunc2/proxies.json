{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "task4-GetProducts": {
      "matchCondition": {
        "route": "/openhack-19/task4/GetProducts"
      },
      "backendUri": "https://serverlessohlondonproduct.azurewebsites.net/api/GetProducts"
    },
    "task4-GetProduct": {
      "matchCondition": {
        "route": "/openhack-19/task4/GetProduct/{productId}"
      },
      "backendUri": "https://serverlessohlondonproduct.azurewebsites.net/api/GetProduct?productId={productId}"
    },
    "task4-GetUser": {
      "matchCondition": {
        "route": "/openhack-19/task4/GetUser/{userId}"
      },
      "backendUri": "https://serverlessohlondonuser.azurewebsites.net/api/GetUser?userId={userId}"
    },
    "task4-CreateRating": {
      "matchCondition": {
        "route": "/openhack-19/task4/CreateRating"
      },
      "backendUri": "https://task2-api-evaluation.azurewebsites.net/api/CreateRating"
    },
    "task4-GetRating": {
      "matchCondition": {
        "route": "/openhack-19/task4/GetRating/{productId}"
      },
      "backendUri": "https://task2-api-evaluation.azurewebsites.net/api/GetRating?productId={productId}"
    },
    "task4-GetRatings": {
      "matchCondition": {
        "route": "/openhack-19/task4/GetRatings/{userId}"
      },
      "backendUri": "https://task2-api-evaluation.azurewebsites.net/api/GetRatings?userId={userId}"
    }
  }
}